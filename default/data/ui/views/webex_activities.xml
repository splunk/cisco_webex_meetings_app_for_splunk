<form>
  <label>WebEx - Activities</label>
  <fieldset submitButton="false">
    <input type="time" token="time_tok">
      <label></label>
      <default>
        <earliest>-7d@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Webex Meeting Geo Info Overview</title>
      <table>
        <search>
          <query>`webex_index` sourcetype="cisco:webex:meetings:history:meetingattendeehistory"  | stats values(name) as Username, values(participantType) as "Participant Type", values(clientAgent) as "Client Agent", values(ipAddress) as IP by confName</query>
          <earliest>$time_tok.earliest$</earliest>
          <latest>$time_tok.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Daily Meeting Overview</title>
      <table>
        <search>
          <query>`webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory"| eval Date=strftime(_time, "%m/%d/%Y") | stats sum(totalParticipants) as "Total Number of Participants" by Date
| join Date [search `webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory" | eval Date=strftime(_time, "%m/%d/%Y") | stats count(confID) as "Total Number of Sessions" by Date]
| table Date , "Total Number of Sessions", "Total Number of Participants"</query>
          <earliest>$time_tok.earliest$</earliest>
          <latest>$time_tok.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>WebEx Overview</title>
      <table>
        <search>
          <query>`webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory"  confID=* totalParticipants=* | bin span=1d _time
| eval Conference = confName . " (" . totalParticipants . " users)" 
| eval threshold=1 
| sort _time, - totalParticipants 
| where totalParticipants &gt; threshold 
| fields - threshold 
| stats list(Conference) as "Large Conferences" by _time
| join _time [search `webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory"| bin span=1d _time | stats sum(totalParticipants) as "Total Number of Participants" by _time]
| join _time [search `webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory" | bin span=1d _time | stats count(confID) as "Total Number of Sessions" by _time]
| table _time , "Total Number of Sessions", "Total Number of Participants", "Large Conferences"</query>
          <earliest>$time_tok.earliest$</earliest>
          <latest>$time_tok.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="Total Number of Participants">
          <colorPalette type="minMidMax" maxColor="#53A051" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
        <format type="color" field="Total Number of Sessions">
          <colorPalette type="minMidMax" maxColor="#53A051" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
        <format type="color" field="Large Conferences">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>List of Large Conferences</title>
      <table>
        <search>
          <query>`webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory" confID=* totalParticipants=*  
| table _time confName hostName totalParticipants
| eval threshold=1 
| where totalParticipants &gt; threshold
| fields - threshold 
| sort _time, - totalParticipants</query>
          <earliest>$time_tok.earliest$</earliest>
          <latest>$time_tok.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">preview</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="totalParticipants">
          <colorPalette type="minMidMax" maxColor="#62B3B2" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Prediction for number of Conference</title>
      <chart>
        <search>
          <query>`webex_index` sourcetype="cisco:webex:meetings:history:meetingusagehistory" confID=* totalParticipants=* duration=* 
| timechart count(confID) span=1d as confID | predict confID</query>
          <earliest>0</earliest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">none</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">column</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.mode">standard</option>
        <option name="charting.legend.placement">right</option>
        <option name="charting.lineWidth">2</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </chart>
    </panel>
  </row>
</form>
